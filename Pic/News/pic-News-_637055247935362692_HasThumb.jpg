    <script type="text/javascript">
        function CountItemsInCart() {            
            jQuery.ajax({
                url: "http://thietbichunganh.com/cms/display/Product/Controls/ProductCartAspx.aspx",
            type: "POST",
            dataType: "json",
            data: {
                "action": "CountItemsInCart",
            },
            success: function (res) {                
                $('#slcart').html(res[0]);
            },
            error: function (error) { //Lỗi xảy ra
                loading(false);
            }
        });
    }
    CountItemsInCart();
</script>
