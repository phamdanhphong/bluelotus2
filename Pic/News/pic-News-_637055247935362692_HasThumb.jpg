    <script type="text/javascript">
            $(window)
            .load(function () {
                $(".khungAnhCrop img")
                    .each(function () {
                        $(this)
                            .removeClass("wide tall")
                            .addClass((this.width / this.height > $(this).parent().width() / $(this).parent().height())
                                ? "wide"
                                : "tall");
                    });
            });
