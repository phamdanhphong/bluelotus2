animation: icon-luc-lac 1.2s infinite ease-in-out;
}

.btn-call-fixed .icon-wrap .border-1 {
width: 100px;
height: 100px;
position: absolute;
z-index: 1;
top: -25px;
left: -25px;
border: solid 1px red;
border-radius: 50%;
animation: vien-lon-zoom 1.2s infinite ease-in-out;
}

.btn-call-fixed .icon-wrap .border-2 {
width: 70px;
height: 70px;
position: absolute;
z-index: 1;
top: -10px;
